	],
	"auth": {
		"type": "oauth2",
		"oauth2": [
			{
				"key": "clientSecret",
				"value": "",
				"type": "string"
			},
			{
				"key": "clientId",
				"value": "",
				"type": "string"
			},
			{
				"key": "authUrl",
				"value": "",
				"type": "string"
			},
			{
				"key": "accessTokenUrl",
				"value": "",
				"type": "string"
			},
			{
				"key": "tokenType",
				"value": "",
				"type": "string"
			},
			{
				"key": "accessToken",
				"value": "{{access_token}}",
				"type": "string"
			},
			{
				"key": "addTokenTo",
				"value": "header",
				"type": "string"
			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Initialize variables",
					"var now=new Date();",
					"var now_addXXdays=new Date();",
					"",
					"let requestName=pm.info.requestName;",
					"let n = 5;",
					" ",
					"if (requestName.startsWith(\"SKIP\")){",
					"   requestName = requestName.slice(n);",
					"  pm.globals.set(\"request.label\", requestName);",
					"}",
					"pm.globals.set(\"timestamp.iso\", now.toISOString());",
					"pm.globals.set(\"request.label\", requestName);",
					"now_addXXdays.setDate(now.getDate() - 15);",
					"pm.environment.set(\"timestamp.iso.remove.15days\", now_addXXdays.toISOString());",
					"",
					"var now_addXXdays=new Date();",
					"now_addXXdays.setDate(now.getDate() - 1);",
					"pm.environment.set(\"timestamp.iso.remove.1days\", now_addXXdays.toISOString());",
					"",
					"var now_addXXdays=new Date();",
					"now_addXXdays.setDate(now.getDate() + 30);",
					"pm.environment.set(\"timestamp.iso.add.10days\", now_addXXdays.toISOString());",
					"// add a 30 day",
					"",
					"var now_addXXdays=new Date();",
					"now_addXXdays.setDate(now.getDate() + 30);",
					"pm.environment.set(\"timestamp.iso.add.30days\", now_addXXdays.toISOString());",
					"// add a 60 day",
					"",
					"var now_addXXdays=new Date();",
					"now_addXXdays.setDate(now.getDate() + 60);",
					"pm.environment.set(\"timestamp.iso.add.60days\", now_addXXdays.toISOString());",
					"",
					"var parts = requestName.split(\"|\");",
					"",
					"var action = parts[0];",
					"var entity = parts[1];",
					"var suffix = parts[2];",
					"var status = parts[3];",
					"",
					"for(let i=0; i<parts.length; i++) {",
					"    pm.globals.set(\"wait\", 0);",
					"    pm.globals.set(\"request.label.\"+i, parts[i]);",
					"",
					"    if(parts[i].indexOf(\"=\") != -1 ){",
					"        propName = parts[i].split(\"=\")[0].trim();",
					"        propValue = parts[i].split(\"=\")[1];",
					"        pm.globals.set(propName, propValue);",
					"    }",
					"}",
					"pm.variables.set(\"action\", action);",
					"",
					"pm.variables.set(\"entity\", entity);",
					"pm.variables.set(\"entityRead\", entity.charAt(0).toLowerCase() + entity.slice(1));",
					"pm.variables.set(\"codeIdentifier\", \"code\");",
					"pm.variables.set(\"haveDescription\", true);",
					"pm.variables.set(\"haveCode\", true);",
					"",
					"pm.variables.set(\"suffix\", suffix);",
					"var entityCode = pm.collectionVariables.get(\"testCode\")+\"_\"+suffix;",
					"pm.variables.set(\"entityCode\", entityCode);",
					"pm.variables.set(\"description\", pm.collectionVariables.get(\"testDescription\"));",
					"",
					"pm.variables.set(\"messageEql\", true);",
					"pm.variables.set(\"status\", status);",
					"",
					"",
					"switch(status){",
					"    case \"SUCCESS\": {",
					"        pm.variables.set(\"code\", 200);",
					"        (action.includes(\"Read\") ? pm.variables.set(\"message\", ) : pm.variables.set(\"message\", \"\"));",
					"        pm.variables.set(\"status\", status);",
					"        pm.variables.set(\"errorCode\", );",
					"        break;        ",
					"    };",
					"    case \"ENTITY_ALREADY_EXISTS_EXCEPTION\": {",
					"        pm.variables.set(\"code\", 409);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        pm.variables.set(\"message\",entity+\" with code=\"+entityCode+\" already exists.\");",
					"        break;",
					"    };",
					"    case \"ENTITY_DOES_NOT_EXISTS_EXCEPTION\": {",
					"        pm.variables.set(\"code\", 404);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        (action==\"Delete\"",
					"        ? pm.variables.set(\"message\", entity+\" with code=\"+entityCode+\" does not exists.\")",
					"        : pm.variables.set(\"message\", entity+\" with code=NOT_EXIST does not exists.\"));",
					"        break;",
					"    };",
					"    case \"MISSING_PARAMETER\":{",
					"        pm.variables.set(\"code\", 400);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    };",
					"    case \"INVALID_PARAMETER\": {",
					"        pm.variables.set(\"code\", 400);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        pm.variables.set(\"messageEql\", false);",
					"        break;",
					"    };",
					"    case \"INTERNAL_SERVER_ERROR\":{",
					"        pm.variables.set(\"code\", 500);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", );",
					"        pm.variables.set(\"errorCode\", );",
					"        break;",
					"    }",
					"    case \"GENERIC_API_EXCEPTION\":{",
					"        pm.variables.set(\"code\", 500);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", );",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    }",
					"    case \"BUSINESS_API_EXCEPTION\":{",
					"        pm.variables.set(\"code\", 500);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    }",
					"    case \"DELETE_REFERENCED_ENTITY_EXCEPTION\":{",
					"        pm.variables.set(\"code\", 403);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    }",
					"    default:{",
					"        pm.variables.set(\"code\", 400);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status); ",
					"    }",
					"}",
					"// Manage token auth",
					"var token = pm.variables.get(\"access_token\");",
					"var refresh_token = pm.variables.get(\"refresh_token\");",
					"",
					"//console.log(\"token=\"+token);",
					"",
					"if(!token || token === \"\"||!pm.environment.get('accessTokenExpiry')",
					"          ||pm.environment.get('accessTokenExpiry') <= (new Date()).getTime()) {",
					"    console.log(\"token is not defined\");",
					"    // Set some environment variables to use in the access token request. ",
					"    var client_id = pm.environment.get(\"client.id\");",
					"    var client_secret = pm.environment.get(\"client.secret\");",
					"    var opencell_url = pm.environment.get(\"opencell.base\");",
					"    //can use an external token url",
					"    ",
					"    var keycloak_token_url = pm.environment.get(\"keycloak.token.url\");",
					"    console.log(\"keycloak_token_url: \"+ keycloak_token_url);",
					"    ",
					"    if(!keycloak_token_url || keycloak_token_url === \"\") {",
					"        keycloak_token_url = opencell_url + '/auth/realms/opencell/protocol/openid-connect/token';",
					"        pm.globals.set(\"keycloak_token_url\", keycloak_token_url);",
					"        console.log(\"new keycloak_token_url: \"+ keycloak_token_url);",
					"    }",
					"    ",
					"",
					"",
					"    var username = pm.environment.get(\"opencell.username\");",
					"    var password = pm.environment.get(\"opencell.password\");",
					"    var CryptoJS = require(\"crypto-js\")",
					"    // Encrypt the Client ID and Secret in base64 ",
					"    var rawStr = CryptoJS.enc.Utf8.parse(client_id + ':' + client_secret)",
					"    var client_cred = CryptoJS.enc.Base64.stringify(rawStr)",
					"    // Now let's get a new oauth2 access_token and set it in a variable ",
					"",
					"    pm.sendRequest(",
					"        { ",
					"            url: keycloak_token_url, ",
					"           //      url: opencell_url + '/auth/realms/opencell/protocol/openid-connect/token', ",
					"            method: 'POST', ",
					"            header: ",
					"            { ",
					"                //'Authorization': 'Basic ' + client_cred, ",
					"                'Cache-Control': 'no-cache', ",
					"                'Content-Type': 'application/x-www-form-urlencoded' ",
					"            }, body: ",
					"            { ",
					"                mode: 'urlencoded', ",
					"                urlencoded: ",
					"                [",
					"                    { ",
					"                        key: \"client_id\", ",
					"                        value: client_id ",
					"                    }, ",
					"                    { ",
					"                        key: \"username\", ",
					"                        value: username ",
					"                    },",
					"                    { ",
					"                        key: \"password\", ",
					"                        value: password ",
					"                    },",
					"                    { ",
					"                        key: \"client_secret\", ",
					"                        value: client_secret",
					"                    },",
					"                    {",
					"                        key: \"refresh_token\",",
					"                        value: refresh_token",
					"                    },",
					"                    {",
					"                        key: \"grant_type\",",
					"                         value: 'password'",
					"                    }",
					"                ] ",
					"            } ",
					"        }, ",
					"        function (err, response) { ",
					"           console.log(\"AUTH_RETURN_CODE\", err);",
					"           console.log(\"AUTH_RESPONSE\", response);",
					"            const jsonResponse = response.json();",
					"            console.log(\"Saving token to variables\");",
					"            pm.environment.set(\"access_token\", jsonResponse.access_token);",
					"            var expiryDate = new Date();",
					"                  //Saving the expiry date = date + 300 seconds (5 minutes)",
					"                  expiryDate.setSeconds(expiryDate.getSeconds() + jsonResponse.expires_in);",
					"                  pm.environment.set('accessTokenExpiry', expiryDate.getTime());",
					"                  //Saving the refresh token",
					"                  pm.environment.set('refresh_token',  jsonResponse.refresh_token);",
					"        }",
					"        );",
					"} else{",
					"    //console.log(\"token already exists\");",
					"}",
					"",
					"// Setup Function library",
					"utils = {",
					"  sleep: function(seconds) {",
					"    console.log(\"Waiting for \" + pm.variables.get(\"request.label.0\") + \" for \" + seconds + \" seconds...\");",
					"    setTimeout(function () { }, seconds * 1000);",
					"  }",
					"};",
					"",
					"",
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"var jsonData = pm.response.json();",
					"",
					"pm.test(request.name + \" - Check Code\", function () {",
					"    pm.response.to.have.status(pm.variables.get(\"code\"));",
					"});",
					"",
					"pm.test(request.name + \" - Check Status\", function () {",
					"    pm.expect((jsonData.actionStatus ? jsonData.actionStatus.status : jsonData.status)).to.eql(pm.variables.get(\"status\"));",
					"});",
					"",
					"pm.test(request.name + \" - Check ErrorCode\", function () {",
					"    pm.expect((jsonData.actionStatus ? jsonData.actionStatus.errorCode : jsonData.errorCode)).to.eql(pm.variables.get(\"errorCode\"));",
					"});",
					"",
					"",
					"pm.test(request.name + \" - Check Message\", function () {",
					"    if(pm.variables.get(\"messageEql\")){",
					"        pm.expect((jsonData.actionStatus ? jsonData.actionStatus.message : jsonData.message)).to.eql(pm.variables.get(\"message\"));",
					"    }",
					"    else{",
					"        pm.expect((jsonData.actionStatus ? jsonData.actionStatus.message : jsonData.message)).to.contains(pm.variables.get(\"message\"));",
					"    }",
					"});",
					"/*",
					"if(pm.variables.get(\"action\").includes(\"Read\")){",
					"    if(pm.variables.get(\"haveCode\")){",
					"        pm.test(request.name + \" - Code is equal \" + pm.variables.get(\"entityCode\"), function () {",
					"        pm.expect(jsonData[pm.variables.get(\"entityRead\")][pm.variables.get(\"codeIdentifier\")]).to.eql(pm.variables.get(\"entityCode\"));",
					"        });",
					"    }",
					"    if(pm.variables.get(\"haveDescription\")){",
					"        pm.test(request.name + \" - Description is equal \" + pm.variables.get(\"description\")+(pm.variables.get(\"action\").includes(\"updated\") ? \" updated\": \"\"), function () {",
					"        pm.expect(jsonData[pm.variables.get(\"entityRead\")].description).to.eql(pm.variables.get(\"description\")+(pm.variables.get(\"action\").includes(\"updated\") ? \" updated\": \"\"));",
					"        });",
					"    }",
					"}",
					"*/",
					""
				]
			}
		}
