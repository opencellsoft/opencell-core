		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>FinanceWs</web-resource-name>
			<url-pattern>/FinanceWs</url-pattern>
			<http-method>POST</http-method>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>Invoice4_2Ws</web-resource-name>
			<url-pattern>/Invoice4_2Ws</url-pattern>
			<http-method>POST</http-method>
		</web-resource-collection>
