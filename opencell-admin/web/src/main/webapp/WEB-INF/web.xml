		</web-resource-collection>
		<!-- <web-resource-collection>
			<web-resource-name>WorkflowWs</web-resource-name>
			<url-pattern>/WorkflowWs</url-pattern>
			<http-method>POST</http-method>
		</web-resource-collection> -->
		<web-resource-collection>
			<web-resource-name>GenericWorkflowWs</web-resource-name>
			<url-pattern>/GenericWorkflowWs</url-pattern>
			<http-method>POST</http-method>
		</web-resource-collection>
		<auth-constraint>
			<role-name>apiAccess</role-name>
		</auth-constraint>
	</security-constraint>
	<login-config>
		<auth-method>KEYCLOAK</auth-method>
		<realm-name>opencell</realm-name>
	</login-config>
	<security-role>
		<role-name>apiAccess</role-name>
	</security-role>
	<security-role>
		<role-name>guiAccess</role-name>
	</security-role>
	<error-page>
		<error-code>403</error-code>
		<location>/errors/403.xhtml</location>
	</error-page>
	<error-page>
		<error-code>401</error-code>
		<location>/errors/403.xhtml</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/errors/404.xhtml</location>
	</error-page>
<!-- 	<error-page> -->
<!-- 		<error-code>500</error-code> -->
<!-- 		<location>/errors/500.xhtml</location> -->
<!-- 	</error-page> -->
    <error-page>
        <exception-type>javax.faces.application.ViewExpiredException</exception-type>
        <location>/errors/expired.xhtml</location>
    </error-page>     
    <error-page><!--fix for WF17-->
        <exception-type>javax.enterprise.context.NonexistentConversationException</exception-type>
        <location>/errors/expired.xhtml</location>
    </error-page> 
    <error-page>
        <exception-type>java.lang.Exception</exception-type>
        <location>/errors/500.xhtml</location>
    </error-page>
           
</web-app>