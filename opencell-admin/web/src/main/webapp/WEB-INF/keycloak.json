{
	"realm": "${opencell.keycloak.realm}",
	"auth-server-url": "${opencell.keycloak.url:http://localhost:8080/auth}",
	"ssl-required": "${opencell.keycloak.ssl:none}",
	"resource": "${opencell.keycloak.client:opencell-web}",
	"credentials": {
		"secret": "${opencell.keycloak.secret}"
	},
	"use-resource-role-mappings": true,
	"enable-basic-auth": "${opencell.keycloak.basic-auth:true}",
	"principal-attribute": "preferred_username",
	"proxy-url": "${opencell.keycloak.proxy-url:}",
	"enable-cors": "${opencell.keycloak.enable-cors:false}",
	"cors-allowed-methods": "${opencell.keycloak.cors-allowed-methods:POST, PUT, DELETE, GET, PATCH}",
	"register-node-at-startup": "${opencell.keycloak.register-node-at-startup:false}",
	"register-node-period": 600,
	"policy-enforcer": {
		"paths": [
			{
				"path": "/resources/*",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/frontend/*",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/api/rest/v2/version",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/about.jsf",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/about.xhtml",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/javax.faces.resource/*",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/stylesheet/*",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/errors/*",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/favicon.ico",
				"enforcement-mode": "DISABLED"
			},
			{
				"path": "/inbound/*",
				"enforcement-mode": "DISABLED"
			}		
		],
		"path-cache": {
			"lifespan": 0,
			"max-entries": 0
		},
		"http-method-as-scope": true
	}
}